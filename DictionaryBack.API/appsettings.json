{
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "Properties": {
      "ApplicationName": "Serilog.WebApplication"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "WordsContext": "Host=localhost;Database=dict;Username=postgres;Password=1221"
  },
  "DictionaryApiSettings": {
    "MaxWordsInRequest": 1000,
    "RepetitionSetSize": 30
  }
}
